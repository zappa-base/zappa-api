    "postinstall": "npm run build:all",
    "start:debug": "node --inspect dist/index.js",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 0",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
